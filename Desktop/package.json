{
  "name": "hentai-web-windows",
  "version": "1.3.8",
  "license": "MIT",
  "author": "Der_Googler",
  "main": "src/main.js",
  "build": {
    "appId": "com.dergoogler.hentaiweb.windows",
    "asar": true,
    "npmRebuild": false,
    "compression": "maximum",
    "extends": null,
    "productName": "Hentai Web Windows",
    "copyright": "Copyright Â© 2022 ${author}",
    "win": {
      "icon": "/ic_launcher.png",
      "target": [
        "NSIS",
        "zip"
      ]
    }
  },
  "scripts": {
    "build-dev": "electron .",
    "pack": "electron-builder --dir",
    "build-prod": "electron-builder",
    "build-typescript": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src"
  },
  "repository": "https://github.com/DerGoogler/Hentai-Web/Windows",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^7.32.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@electron/get": "^1.13.1",
    "@electron/remote": "^2.0.1",
    "custom-electron-titlebar": "^3.2.10",
    "discord-rich-presence": "^0.0.8",
    "dragscroll": "^0.0.8",
    "electron-google-analytics": "^1.0.2",
    "electron-preferences": "^2.5.0",
    "electron-store": "^8.0.1",
    "electron-window-shortcut": "^1.0.3",
    "mousetrap": "^1.6.5"
  }
}
