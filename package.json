  "scripts": {
    "start": "bun run start:dev",
    "start:dev": "webpack-dev-server --open --config webpack.dev.ts",
    "start:prod": "webpack-dev-server --open --config webpack.prod.ts",
    "web:dev": "bun run util:dep && webpack --config webpack.dev.ts && bun run util:copy",
    "web:prod": "bun run util:dep && bun run web:dev && webpack --config webpack.prod.ts && bun run util:copy",
    "app:dev": "bun run web:dev && cd ../Android && ./gradlew installDebug",
    "util:dep": "bun dep.ts > ./src/util/dep.json && cd ./../Android && ./gradlew app:printSolvedDepsTreeInJson",
    "util:copy": "bun run util:rm && cp -rf ./build/www/ ./../Android/app/src/main/assets",
    "util:rm": "rm -rf ./../Android/app/src/main/assets/www/"
  },
