{
  "compilerOptions": {
    "outDir": "./dist/",
    "typeRoots": ["src/typings/global.d.ts"],
    "sourceMap": true,
    "declaration": false,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "es6",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "target": "es5",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@DerGoogler/native": ["./native/index.ts"],
    }
  },
  "include": ["./src/**/*"]
}
